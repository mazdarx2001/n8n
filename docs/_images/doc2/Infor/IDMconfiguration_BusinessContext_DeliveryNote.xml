<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns="http://infor.com/daf">
    <bcModels>
        <bcModel>
            <name>InforPurchaseOrder:::DELIVERY_NOTE</name>
            <xquery>/DELIVERY_NOTE[@MDS_EntityType = "{entityType}" AND @MDS_id1 = "{id1}"]</xquery>
            <queries>
                <query>
                    <entities>DELIVERY_NOTE</entities>
                    <useTextSearch>false</useTextSearch>
                    <arguments>
                        <argument>
                            <key>MDS_EntityType</key>
                            <operator>=</operator>
                            <value>{entityType}</value>
                            <type>1</type>
                            <logicalType>AND</logicalType>
                        </argument>
                        <argument>
                            <key>MDS_id1</key>
                            <operator>=</operator>
                            <value>{id1}</value>
                            <type>1</type>
                            <logicalType>AND</logicalType>
                        </argument>
                    </arguments>
                </query>
            </queries>
        </bcModel>
    </bcModels>
</configuration>
